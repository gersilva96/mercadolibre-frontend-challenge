{
  "name": "mercadolibre-challenge-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "reinstall-modules": "run-s clean:modules && yarn install",
    "clean:modules": "rimraf node_modules yarn.lock",
    "clean:build": "rimraf .next out build",
    "test": "jest",
    "test:watch": "jest --watch",
    "control": "run-s lint format type-check test && yarn audit --production && echo \"\nControl finished!\n\"",
    "lint": "eslint --fix --ignore-path .eslintignore \"src/**/*.+(ts|tsx|js|jsx)\" && stylelint --fix --aei --config .stylelintrc \"src/**/*.+(css|scss|sass)\"",
    "format": "prettier --write --ignore-path .gitignore \"src/**/*.+(ts|tsx|js|jsx)\"",
    "type-check": "tsc",
    "dev": "next dev",
    "build:control": "run-s clean:build control && next build",
    "build": "run-s clean:build && next build",
    "export": "run-s build && next export",
    "start": "next start"
  },
  "dependencies": {
    "@chakra-ui/react": "1.8.3",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@hookform/resolvers": "2.8.8",
    "@reduxjs/toolkit": "1.7.2",
    "axios": "0.26.0",
    "date-fns": "2.28.0",
    "framer-motion": "6.2.6",
    "i18next": "21.6.11",
    "immer": "9.0.12",
    "lodash-es": "4.17.21",
    "next": "12.1.0",
    "next-seo": "5.1.0",
    "postcss-scss": "4.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.27.0",
    "react-i18next": "11.15.4",
    "react-icons": "4.3.1",
    "react-redux": "7.2.6",
    "swiper": "8.0.6",
    "url-join": "4.0.1",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@next/env": "12.1.0",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@types/jest": "27.4.0",
    "@types/lodash-es": "4.17.6",
    "@types/node": "17.0.18",
    "@types/react": "17.0.39",
    "@types/testing-library__jest-dom": "5.14.2",
    "@types/url-join": "4.0.1",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "eslint": "8.9.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-testing-library": "5.0.5",
    "jest": "27.5.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "sass": "1.49.8",
    "stylelint": "14.5.1",
    "stylelint-config-recommended": "7.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.5.0",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.1.0",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  }
}
